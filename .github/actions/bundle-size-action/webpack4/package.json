{
  "private": true,
  "exports": "./dist/app.modern.js",
  "main": "./dist/app.js",
  "module": "./dist/app.module.js",
  "source": "./app.js",
  "unpkg": "./dist/app.umd.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack",
    "precalculate": "$_ run build",
    "calculate": "bundlewatch"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "dist/*.min.js",
        "maxSize": "200kB"
      },
      {
        "path": "dist/amazon-cognito-identity-js.js.min.js",
        "maxSize": "155kB"
      },
      {
        "path": "dist/Amplify.js.min.js",
        "maxSize": "2kB"
      },
      {
        "path": "dist/Amplify+Auth.js.min.js",
        "maxSize": "215kB"
      }
    ]
  },
  "dependencies": {
    "aws-amplify": "unstable"
  },
  "devDependencies": {
    "bundlewatch": "^0.3.1",
    "glob": "^7.1.6",
    "rimraf": "^3.0.2",
    "webpack": "webpack-4",
    "webpack-cli": "^4.3.1"
  }
}
